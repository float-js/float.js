{
  "name": "@float.js/core",
  "version": "2.0.2",
  "description": "Float.js Core - Ultra Modern Web Framework",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "float": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js"
    },
    "./router": {
      "types": "./dist/router/index.d.ts",
      "import": "./dist/router/index.js"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "chokidar": "^3.5.3",
    "esbuild": "^0.25.0",
    "fast-glob": "^3.3.2",
    "mime-types": "^2.1.35",
    "picocolors": "^1.0.0",
    "ws": "^8.19.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/ws": "^8.18.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "float",
    "framework",
    "ssr",
    "ssg",
    "react",
    "modern",
    "edge"
  ],
  "license": "MIT"
}
