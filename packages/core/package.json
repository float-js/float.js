{
  "name": "@float/core",
  "version": "0.1.0",
  "description": "Float.js core framework with CLI, dev server, and SSR",
  "keywords": [
    "float",
    "react",
    "ssr",
    "hmr",
    "framework"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/float-js/float-js.git",
    "directory": "packages/core"
  },
  "license": "MIT",
  "author": "float-js",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "float": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "esbuild": "^0.19.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ws": "^8.16.0",
    "chokidar": "^3.5.3",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/ws": "^8.5.10",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
