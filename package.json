{
  "name": "float-js-monorepo",
  "version": "0.0.0",
  "private": true,
  "description": "The React framework for the AI era",
  "keywords": [
    "react",
    "framework",
    "ssr",
    "streaming",
    "hmr",
    "router",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/float-js/float-js.git"
  },
  "license": "MIT",
  "author": "float-js",
  "workspaces": [
    "packages/*",
    "docs"
  ],
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r --parallel dev",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "pnpm -r type-check",
    "clean": "pnpm -r clean && rm -rf node_modules"
  },
  "devDependencies": {
    "@types/node": "^18.19.3",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
